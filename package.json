{
  "name": "ts-backend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "npx tsx src/index.ts",
    "book": "cp ../booking_system/config/config.library.author.book.json config/config.json && npx tsx src/index.ts",
    "desk": "cp ../booking_system/config/config.work.floor.desk.json config/config.json && npx tsx src/index.ts",
    "carpark": "cp ../booking_system/config/config.carparks.json config/config.json && npx tsx src/index.ts",
    "todo": "cp config/config.todo.json config/config.json && npx tsx src/index.ts",
    "allitems": "concurrently --names \"BOOK,DESK\" --prefix-colors \"bgGreen.bold,bgMagenta.bold\" \"npm run book\" \"npm run desk\"",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test_server": "cp config/config.test.json config/config.json && npx tsx src/index.ts",
    "test_script": "sleep 3 && cd tests && bash uritest.sh",
    "test": "concurrently --names \"SERVER, SCRIPT\" --prefix-colors \"bgGreen.bold,bgMagenta.bold\" \"npm run test_server\" \"npm run test_script\""
  },
  "dependencies": {
    "concurrently": "^8.2.0",
    "save-dev": "^0.0.1-security",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.3",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "cors": "^2.8.5",
    "eslint": "^8.43.0",
    "express": "^4.18.2",
    "mongoose": "^7.3.0",
    "tslint": "^6.1.3",
    "typescript": "^5.1.3"
  }
}
